#: ../includes/class-order.php:18 ../includes/class-order.php:36
msgid "Picked up"
msgstr "Upplockad"

#: ../includes/class-order.php:23
#, php-format
msgid "Picked up <span class=\"count\">(%s)</span>"
msgid_plural "Picked up <span class=\"count\">(%s)</span>"
msgstr[0] "Upplockad <span class=\"count\">(%s)</span>"
msgstr[1] "Upplockad <span class=\"count\">(%s)</span>"

#: ../includes/class-order.php:54
#, php-format
msgid "Urb-it delivery time: %s"
msgstr "Urb-it leveranstid: %s"

